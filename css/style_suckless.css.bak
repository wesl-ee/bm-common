@font-face {
	font-family: ProggySquare;
	src: url('/css/ProggySquare.ttf');
}

body {
	background-color: #BBF3FF;
	font-family: ProggySquare;
	color: #B0475A;
}

table {
//	display: block;
//	position: absolute;
	margin: auto;
	border-radius: 25px;
	border-collapse:collapse;
	border: 0px solid black;
	text-align: center;
	background-color: #FFD4E5;
	padding: 10px;
	transform: translateY(30%);
}
.entry {
        text-align: left;
	word-wrap: break-word;
	position:relative;
	line-height:120%;
}
.frame {
        width: 50%;
        margin: auto;
	left: 0;
	right: 0;
        border-radius: 25px;
        border-collapse:collapse;
        border: 0px solid black;
        background-color: #FFD4E5;
        padding: 10px;
	position:absolute;
}
.center {
	padding: 20px;
	width:35%;
	overflow:hidden;
	margin:auto;
	display:block;
}	
.centeredEntry {
	word-wrap:break-word;
	position:relative;
	text-align: center;
	line-height:120%;
}
.fileList {
        text-align: left;
        width: 100%;
        position:relative;
        line-height:120%;
        white-space: nowrap;
        overflow: scroll;
        text-overflow: ellipsis;
}
.logout {
	border-radius: 0px 0px 25px 25px;
	background-color: #FFD4E5;
	padding: 5px 45px 15px 45px;
	position: absolute;
	top: 0px;
	right: 10%;
	text-align:center;
}
th, td {
	padding: 10px;
	color: #B0475A;
	font-family: ProggySquare;
}
a {
	color: #FD7F96;
	text-decoration: none;
	
}
a:hover {
	color: #5ABFCF;
}
#mascot {
	filter:none;
        -webkit-filter: grayscale(100%);
        -moz-filter:    grayscale(100%);
        -ms-filter:     grayscale(100%);
        -o-filter:      grayscale(100%);
        transition: all 300ms ease;
	display: block;
	position: absolute;
	margin: 0;
	max-width:25%;
	max-height:50%;
	bottom: 0px;
	right: 10%;

}
#mascot:hover {
        filter: none;
        -webkit-filter: grayscale(0%);
        -moz-filter:    grayscale(0%);
        -ms-filter:     grayscale(0%);
        -o-filter:      grayscale(0%);
}

input[type=text] {
	bakground-color: #ffffff;
	opacity: 0.5;
	border-style: none;
}
input[type=password] {
	background-color: #ffffff;
	opacity: 0.5;
	border-style: none;
}
input[type=submit] {
	background-color: #ffffff;
	border-style: none;
}
.upperRight {
	z-index:1;
	position:absolute;
	text-align:right;
	top:10px;
	right:20px;
}
h1 {
	text-overflow: ellipsis;
	overflow: hidden;
}
#gallery {
	margin:auto;
	max-width:100%;
	max-height:100%;
	border-collapse:collapse;
	border-radius:25px 25px 25px 25px;
}
.footer {
	margin:auto;
	width:30%;
	position: relative;
	padding: 1rem;
	border-radius: 0px 0px 25px 25px;
	background-color:#FFD4E5;
	text-align:center;
}
img {
	image-orientation: from-image;
}